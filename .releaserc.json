{
  "branches": ["main"],
  "plugins": [
    [
      "@semantic-release/commit-analyzer",
      {
        "preset": "angular"
      }
    ],
    [
      "@semantic-release/exec", 
      {
        "publishCmd": "poetry version ${nextRelease.version}"
      }
    ],
    "@semantic-release/github"
  ]
}
